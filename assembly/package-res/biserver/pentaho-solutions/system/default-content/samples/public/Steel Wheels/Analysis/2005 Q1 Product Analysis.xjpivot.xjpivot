      <component-definition> 
        <title><![CDATA[2005 Q1 Product Analysis]]></title>  
        <viewer>plugin/jpivot/Pivot</viewer>  
        <model><![CDATA[mondrian:/SteelWheels]]></model>  
        <options> 
          <personal/>  
          <cube-nav/>  
          <mdx-edit/>  
          <sort-conf/>  
          <spacer/>  
          <level-style/>  
          <hide-spans/>  
          <properties/>  
          <non-empty/>  
          <swap-axes/>  
          <spacer/>  
          <drill-member/>  
          <drill-position/>  
          <drill-replace/>  
          <drill-thru/>  
          <spacer/>  
          <chart/>  
          <chart-conf/>  
          <spacer/>  
          <print-conf/>  
          <print-pdf/>  
          <spacer/>  
          <excel/> 
        </options>  
        <query><![CDATA[with set prods as 'TopCount([Product].[Product].Members, 10, [Measures].[Sales])'
  member [Measures].[Sales 2004-Q1] as '([Measures].[Sales], ParallelPeriod([Time].[Years], 1, [Time].CurrentMember))'
  member [Measures].[2004-Q1] as 'IIf((([Measures].[Sales] > 0) AND ([Measures].[Sales 2004-Q1] > 0)), (([Measures].[Sales] / ([Measures].[Sales], ParallelPeriod([Time].[Years], 1, [Time].CurrentMember))) - 1), NULL)', FORMAT_STRING = IIf(([Measures].[2004-Q1] < 0), "|0.0%|style=red", "|0.0%|style=green")
select NON EMPTY (Descendants([Markets].[All Markets], 1, SELF_AND_BEFORE) * {[Measures].[Sales], [Measures].[2004-Q1]}) ON COLUMNS,
  NON EMPTY [prods] ON ROWS
from [SteelWheelsSales]
where ([Order Status].[Shipped], [Time].[2005].[QTR1])
]]></query>  
