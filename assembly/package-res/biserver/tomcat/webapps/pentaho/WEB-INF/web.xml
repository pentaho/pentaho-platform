	</filter-mapping>

	<filter-mapping>
		<filter-name>Proxy Trusting Filter</filter-name>
		<url-pattern>/SubscriptionAdminService</url-pattern>
	</filter-mapping>

	<filter-mapping>
		<filter-name>Proxy Trusting Filter</filter-name>
