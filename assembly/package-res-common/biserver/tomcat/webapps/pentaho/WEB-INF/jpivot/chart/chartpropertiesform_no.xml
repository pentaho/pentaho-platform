<?xml version="1.0" encoding="iso-8859-1"?>

<xform style="manual">
<table border="1" cellspacing="0" cellpadding="2">

 <tr>
  <th colspan="2" class="xform-title">
   <table border="0" cellspacing="0" cellpadding="0" width="100%">
    <tr>
     <th align="left" class="xform-title">
      Diagramoppsett
     </th>
     <td align="right" class="xform-close-button">
      <imgButton src="wcf/form/cancel.png" action="revert" label="Lukk" hide="true" handler="com.tonbeller.wcf.form.ButtonHandler"/>
     </td>
    </tr>
   </table>
  </th>
 </tr>

 <tr>
  <td>Diagramtype</td>
  <td>
   <listBox1 type="int" modelReference="chartType">
	<listItem value="1" label="Vertikal stolpe"/>
	<listItem value="2" label="Vertikal stolpe 3D"/>
	<listItem value="3" label="Horisontal stolpe"/>
	<listItem value="4" label="Horisontal solpe 3D"/>
	<listItem value="5" label="Stablet vertikal stolpe"/>
	<listItem value="6" label="Stablet vertikal stolpe 3D"/>
	<listItem value="7" label="Stablet horisontal stolpe"/>
	<listItem value="8" label="Stablet horisontal stolpe 3D"/>
	<listItem value="9" label="Vertikal linje"/>
	<listItem value="10" label="Horisontal linje"/>
	<listItem value="11" label="Vertikalt område"/>
	<listItem value="12" label="Horisontalt område"/>
	<listItem value="13" label="Vertikalt stablet område"/>
	<listItem value="14" label="Horisontalt område"/>
	<listItem value="15" label="Kakediagram etter kolonner"/>
	<listItem value="16" label="Kakediagram etter rader"/>
   </listBox1>
  </td>
 </tr>

 <tr>
  <td>Tillat drilling</td>
  <td>
   <checkBox modelReference="drillThroughEnabled"/>
  </td>
 </tr>

 <tr>
  <td>Diagramtittel</td>
  <td>
   <textField type="string" modelReference="chartTitle" size="30" title="skriv inn en tittel for dette diagrammet"/>
  </td>
 </tr>
 <tr>
  <td>Skrifttype for diagramtittel</td>
  <td>
   <listBox1 type="string" modelReference="fontName" label="Skriftype tittel">
  	<listItem value="SansSerif" label="SansSerif"/>
	<listItem value="Serif" label="Serif"/>
	<listItem value="Monospaced" label="Lik bokstavstørrelse"/>
   </listBox1>
   <listBox1 type="int" modelReference="fontStyle" label="Skriftstil for tittel">
  	<listItem value="0" label="Vanlig"/>
	<listItem value="1" label="Uthevet"/>
	<listItem value="2" label="Kursiv"/>
	<listItem value="3" label="Uthevet kursiv"/>
   </listBox1>
   <listBox1 type="int" modelReference="fontSize" label="Skriftstørrelse på tittel">
  	<listItem value="8" label="8"/>
	<listItem value="10" label="10"/>
	<listItem value="12" label="12"/>
	<listItem value="14" label="14"/>
	<listItem value="16" label="16"/>
	<listItem value="18" label="18"/>
	<listItem value="20" label="20"/>
	<listItem value="22" label="22"/>
	<listItem value="24" label="24"/>
	<listItem value="26" label="26"/>
	<listItem value="28" label="28"/>
	<listItem value="30" label="30"/>
	<listItem value="32" label="32"/>
   </listBox1>
  </td>
 </tr>
 
 <tr>
  <td>Navn på horisontal akse</td>
  <td>
   <textField type="string" modelReference="horizAxisLabel" size="30" title="skriv inn et navn på den horisontale aksen"/>
  </td>
 </tr>
 <tr>
  <td>Navn på vertikal akse</td>
  <td>
   <textField type="string" modelReference="vertAxisLabel" size="30" title="skriv inn et navn på den vertikale aksen"/>
  </td>
 </tr>
 <tr>
  <td>Skrifttype, navn akser</td>
  <td>
   <listBox1 type="string" modelReference="axisFontName">
  	<listItem value="SansSerif" label="SansSerif"/>
	<listItem value="Serif" label="Serif"/>
	<listItem value="Monospaced" label="Monospaced"/>
   </listBox1>
   <listBox1 type="int" modelReference="axisFontStyle">
  	<listItem value="0" label="Standard"/>
	<listItem value="1" label="Uthevet"/>
	<listItem value="2" label="Kursiv"/>
	<listItem value="3" label="Uthevet kursiv"/>
   </listBox1>
   <listBox1 type="int" modelReference="axisFontSize">
  	<listItem value="8" label="8"/>
	<listItem value="10" label="10"/>
	<listItem value="12" label="12"/>
	<listItem value="14" label="14"/>
	<listItem value="16" label="16"/>
	<listItem value="18" label="18"/>
	<listItem value="20" label="20"/>
	<listItem value="22" label="22"/>
	<listItem value="24" label="24"/>
	<listItem value="26" label="26"/>
	<listItem value="28" label="28"/>
	<listItem value="30" label="30"/>
	<listItem value="32" label="32"/>
   </listBox1>
  </td>
 </tr>
 
 <tr>
  <td>Skrifttype akser</td>
  <td>
   <listBox1 type="string" modelReference="axisTickFontName">
  	<listItem value="SansSerif" label="SansSerif"/>
	<listItem value="Serif" label="Serif"/>
	<listItem value="Monospaced" label="Monospaced"/>
   </listBox1>
   <listBox1 type="int" modelReference="axisTickFontStyle">
  	<listItem value="0" label="Standard"/>
	<listItem value="1" label="Uthevet"/>
	<listItem value="2" label="Kursiv"/>
	<listItem value="3" label="Uthevet kursiv"/>
   </listBox1>
   <listBox1 type="int" modelReference="axisTickFontSize">
  	<listItem value="8" label="8"/>
	<listItem value="10" label="10"/>
	<listItem value="12" label="12"/>
	<listItem value="14" label="14"/>
	<listItem value="16" label="16"/>
	<listItem value="18" label="18"/>
	<listItem value="20" label="20"/>
	<listItem value="22" label="22"/>
	<listItem value="24" label="24"/>
	<listItem value="26" label="26"/>
	<listItem value="28" label="28"/>
	<listItem value="30" label="30"/>
	<listItem value="32" label="32"/>
   </listBox1>
   <listBox1 type="int" modelReference="tickLabelRotate">
        <listItem value="0" label="0°"/>
        <listItem value="30" label="30°"/>
        <listItem value="45" label="45°"/>
        <listItem value="90" label="90°"/>
   </listBox1>
  </td>
 </tr>

 <tr>
  <td>Vis etiketter</td>
  <td>
   <checkBox modelReference="showLegend"/>
   <listBox1 type="int" modelReference="legendPosition">
  	<listItem value="1" label="Topp"/>
	<listItem value="3" label="Bunn"/>
	<listItem value="0" label="Venstre"/>
	<listItem value="2" label="Høyre"/>
   </listBox1>
  </td>
 </tr>
 <tr>
  <td>Skrifttype etiketter</td>
  <td>
   <listBox1 type="string" modelReference="legendFontName">
  	<listItem value="SansSerif" label="SansSerif"/>
	<listItem value="Serif" label="Serif"/>
	<listItem value="Monospaced" label="Monospaced"/>
   </listBox1>
   <listBox1 type="int" modelReference="legendFontStyle">
  	<listItem value="0" label="Standard"/>
	<listItem value="1" label="Uthevet"/>
	<listItem value="2" label="Kursiv"/>
	<listItem value="3" label="Uthevet kursiv"/>
   </listBox1>
   <listBox1 type="int" modelReference="legendFontSize">
  	<listItem value="8" label="8"/>
	<listItem value="10" label="10"/>
	<listItem value="12" label="12"/>
	<listItem value="14" label="14"/>
	<listItem value="16" label="16"/>
	<listItem value="18" label="18"/>
	<listItem value="20" label="20"/>
	<listItem value="22" label="22"/>
	<listItem value="24" label="24"/>
	<listItem value="26" label="26"/>
	<listItem value="28" label="28"/>
	<listItem value="30" label="30"/>
	<listItem value="32" label="32"/>
   </listBox1>
  </td>
 </tr>

 <tr>
  <td>Vis filter</td>
  <td>
   <checkBox modelReference="showSlicer"/>
   <listBox1 type="int" modelReference="slicerPosition" label="Plassering filter">
  	<listItem value="0" label="Toppen"/>
	<listItem value="1" label="Bunnen"/>
	<listItem value="3" label="Venstre"/>
	<listItem value="2" label="Høyre"/>
   </listBox1>
   <listBox1 type="int" modelReference="slicerAlignment" label="Justering filter">
	<listItem value="3" label="Venstre"/>
	<listItem value="2" label="Høyre"/>
	<listItem value="4" label="Midten"/>
   </listBox1>
  </td>
 </tr>
 <tr>
  <td>Skrifttype filter</td>
  <td>
   <listBox1 type="string" modelReference="slicerFontName">
  	<listItem value="SansSerif" label="SansSerif"/>
	<listItem value="Serif" label="Serif"/>
	<listItem value="Monospaced" label="Monospaced"/>
   </listBox1>
   <listBox1 type="int" modelReference="slicerFontStyle">
  	<listItem value="0" label="Standard"/>
	<listItem value="1" label="Uthevet"/>
	<listItem value="2" label="Kursiv"/>
	<listItem value="3" label="Uthevet kursiv"/>
   </listBox1>
   <listBox1 type="int" modelReference="slicerFontSize">
  	<listItem value="8" label="8"/>
	<listItem value="10" label="10"/>
	<listItem value="12" label="12"/>
	<listItem value="14" label="14"/>
	<listItem value="16" label="16"/>
	<listItem value="18" label="18"/>
	<listItem value="20" label="20"/>
	<listItem value="22" label="22"/>
	<listItem value="24" label="24"/>
	<listItem value="26" label="26"/>
	<listItem value="28" label="28"/>
	<listItem value="30" label="30"/>
	<listItem value="32" label="32"/>
   </listBox1>
  </td>
 </tr>

 <tr>
  <td>Diagramhøyde</td>
  <td>
   <textField type="posint" modelReference="chartHeight" size="5" title="skriv inn verdi i piksler - standard 300"/>
   &#160;&#160;&#160;Diagrambredde
   <textField type="posint" modelReference="chartWidth" size="5" title="skriv inn verdi i piksler - standard 500"/>
  </td>
 </tr>
 
 <tr>
  <td>Bakgrunn (R, G, B)</td>
  <td>
   <textField type="posint" modelReference="bgColorR" size="4" title="skriv inn verdi for rød (0-255)"/>
   <textField type="posint" modelReference="bgColorG" size="4" title="skriv inn verdi for grønn (0-255)"/>
   <textField type="posint" modelReference="bgColorB" size="4" title="skriv inn verdi for blå (0-255)"/>
  </td>
 </tr>

 <tr>
  <td align="right" colspan="4">
    <button action="validate" label="OK" handler="com.tonbeller.wcf.form.ButtonHandler" hide="true"/>
    <button action="revert" label="Avbryt" handler="com.tonbeller.wcf.form.ButtonHandler" hide="true"/>
  </td>
 </tr>

</table>

</xform>
